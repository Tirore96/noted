$c1 = 1|3|5
$c2 = 2|4|6
$c3 = 3|5|7
$dur = qn  - qn   - hn - qn
$pos = $c1 - $c2  - $c3
$notes = toNotes($dur,$pos)
$con = {octave=4,key=c}
$con2 = {octave=4,key=e}
$m1 = toMusic($notes,$con)
$m2 = toMusic($notes,$con2)
$dur2 = hn  - hn
$pos2 = $c3 - $c2
$notes2 = toNotes($dur2,$pos2)
$m3 = toMusic($notes2,$con2)

main = $m1 - $m2 - $m3

