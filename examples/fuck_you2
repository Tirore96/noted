ocLD = \l d1 -> let n = (l,d1)
                in . | n || n

en = (1,8)
qn = (1,4)
hn = (1,2)

s1 = . - (ocLD g en) -  (ocLD a en) - (ocLD c (3,4))
s2 = . - (ocLD g en) -  (ocLD a en) - (ocLD d (3,4))
s3 = . - (ocLD g en) -  (ocLD a en) - (ocLD f (3,4))

ss = . - s1 - s2 - s3 - s1

ch = . | c| e | g | b
main = (play 2 ss) parallel (play ch)

